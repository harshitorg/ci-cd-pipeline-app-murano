FormatVersion: 2.1.0
Version: 1.0.0
Name: Hiera Data

Parameters:
  key: $key
  value: $value

Body: |
  return putHieraData('{0} {1} test'.format(args.key, args.value)).stdout

Scripts:
  putHieraData:
    Type: Application
    Version: 1.0.0
    EntryPoint: putHieraData.sh
    Files: []
    Options:
      captureStdout: true
      captureStderr: true
      verifyExitcode: true
